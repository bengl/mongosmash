{
  "name": "mongosmash",
  "version": "0.2.0",
  "description": "A JS Harmony MongoDB/NeDB ODM",
  "main": "index.js",
  "scripts": {
    "pretest": "traceur --modules=commonjs --dir src lib",
    "test": "mocha --harmony",
