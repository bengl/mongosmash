  "author": "Bryan English <bryan@bryanenglish.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bengl/mongosmash/issues"
  },
  "dependencies": {
    "observed": "~1.1.0",
    "traceur-runtime": "0.0.59"
  },
  "devDependencies": {
    "co-mocha": "^1.0.3",
    "istanbul": "^0.3.2",
    "mocha": "^2.0.1",
    "mongodb": "~1.4.23",
    "mongoose": "~3.8.18",
    "nedb": "~1.0.0",
    "traceur": "0.0.79"
  }
